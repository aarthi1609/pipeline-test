{
    "family": "mako-tdf-test",
    "containerDefinitions": [
        {
            "name": "pipeline-container",
            "image": "010928215705.dkr.ecr.ap-south-1.amazonaws.com/mako-ecr-test:latest",
            "cpu": 1536,
            "memory": 1536,
            "memoryReservation": 1536,
            "portMappings": [
                {
                    "name": "container-port",
                    "containerPort": 3000,
                    "hostPort": 0,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "essential": true,
            "environment": [
                {
                    "name": "PORT",
                    "value": "${PORT}"
                }
            ],
            "mountPoints": [],
            "volumesFrom": [],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/",
                    "mode": "non-blocking",
                    "awslogs-create-group": "true",
                    "max-buffer-size": "25m",
                    "awslogs-region": "ap-south-1",
                    "awslogs-stream-prefix": "ecs"
                },
                "secretOptions": []
            },
            "systemControls": []
        }
    ],
    "taskRoleArn": "arn:aws:iam::010928215705:role/ecsTaskExecutionRole",
    "executionRoleArn": "arn:aws:iam::010928215705:role/ecsTaskExecutionRole",
    "networkMode": "bridge",
    "volumes": [],
    "placementConstraints": [],
    "compatibilities": [
        "EC2"
    ],
    "requiresCompatibilities": [
        "EC2"
    ],
    "cpu": "1536",
    "memory": "1536",
    "runtimePlatform": {
        "cpuArchitecture": "ARM64",
        "operatingSystemFamily": "LINUX"
    },
    "tags": []
}